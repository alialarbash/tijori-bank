{
  "expo": {
    "name": "tijori-bank",
    "slug": "tijori-bank",
    "scheme": "tijori",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/vaultDoor.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/vaultDoor.png",
      "resizeMode": "contain",
      "backgroundColor": "#0C1A26"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/vaultDoor.png",
        "backgroundColor": "#0C1A26"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.tijoribank"
    },
    "web": {
      "favicon": "./assets/vaultDoor.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-video",
      "expo-audio"
    ]
  }
}
